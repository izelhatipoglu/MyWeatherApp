<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:bottom="5px" android:right="5px" android:left ="5px" android:top ="5px">
        <shape android:shape="rectangle" android:padding="10dp" >
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
            <stroke
                android:color ="@color/white"
                android:width="3dp"/>
            <solid android:color="#33FFFFFF" />
            <corners android:radius="65dp" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="65dp" />

            <padding
                android:bottom="0dp"
                android:left="0dp"
                android:right="0dp"
                android:top="0dp" />
            <stroke
                android:width="6dp"
                android:color="#177879E8" />
        </shape>
    </item>

</layer-list>